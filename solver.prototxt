# The train/test net protocol buffer definition
train_net: "train_test.prototxt"
test_net: "train_test.prototxt"
test_iter: 1
test_interval: 100
base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 1000
display: 10
max_iter: 10100
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "imagenet_siamese_train"
solver_mode: GPU
